{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ShellyDimmer",
  "type": "object",
  "required": [
    "ap_mac_address",
    "dimmer_status",
    "energy1",
    "fw_version",
    "gateway",
    "input1",
    "input2",
    "ip_address",
    "mac_address",
    "mcu_temperature",
    "mode",
    "power1",
    "rssi",
    "topic_name",
    "updated_properties",
    "user_login",
    "user_password",
    "wifi_password",
    "wifi_ssid"
  ],
  "properties": {
    "ap_mac_address": {
      "type": "string"
    },
    "dimmer_status": {
      "type": "integer",
      "format": "int64"
    },
    "energy1": {
      "type": "integer",
      "format": "int64"
    },
    "fw_version": {
      "type": "string"
    },
    "gateway": {
      "type": "string"
    },
    "input1": {
      "type": "boolean"
    },
    "input2": {
      "type": "boolean"
    },
    "ip_address": {
      "type": "string"
    },
    "mac_address": {
      "type": "string"
    },
    "mcu_temperature": {
      "type": "number",
      "format": "double"
    },
    "mode": {
      "type": "integer",
      "format": "int64"
    },
    "power1": {
      "type": "integer",
      "format": "int64"
    },
    "rssi": {
      "type": "integer",
      "format": "int64"
    },
    "topic_name": {
      "type": "string"
    },
    "updated_properties": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "user_login": {
      "type": "string"
    },
    "user_password": {
      "type": "string"
    },
    "wifi_password": {
      "type": "string"
    },
    "wifi_ssid": {
      "type": "string"
    }
  }
}